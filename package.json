{
  "name": "slack-alerts",
  "version": "1.0.11",
  "description": "A slack API wrapper for sending alerts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "test": "vitest"
  },
  "keywords": [
    "slack",
    "alerts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beikeni/slack-alerts.git"
  },
  "homepage": "https://github.com/beikeni/slack-alerts",
  "bugs": {
    "url": "https://github.com/beikeni/slack-alerts/issues"
  },
  "author": "Alessandro Baccini",
  "license": "ISC",
  "packageManager": "pnpm@10.17.0",
  "devDependencies": {
    "tsdown": "0.17.0-beta.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@slack/web-api": "^7.13.0",
    "dotenv": "^17.2.3"
  }
}
